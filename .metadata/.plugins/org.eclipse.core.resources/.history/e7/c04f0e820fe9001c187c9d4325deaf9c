package com.tic.application.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="metas")
public class Metas {
	
	@Id
	@Column(name="numero")
	private String number;
	
	@ManyToOne
    @JoinColumn(name = "meta_ID")
    private User;
	
	
}
